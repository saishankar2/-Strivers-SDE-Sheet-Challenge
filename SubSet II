#include <bits/stdc++.h> 
void sub(int ind,vector<int>&ds,vector<vector<int>>&ans,vector<int> &arr,int n)
{
    ans.push_back(ds);
    for(int i=ind;i<n;i++)
    {
        if(i!=ind&&arr[i]==arr[i-1]) continue;
        ds.push_back(arr[i]);
        sub(i+1,ds,ans,arr,n);
        ds.pop_back();
    }
}
vector<vector<int>> uniqueSubsets(int n, vector<int> &arr)
{
    sort(arr.begin(),arr.end());
    vector<vector<int>>ans;
    vector<int>ds;
    sub(0,ds,ans,arr,n);
    return ans;
}
